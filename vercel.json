{
  "rewrites": [
    {
      "source": "/mlb-api/:path*",
      "destination": "https://statsapi.mlb.com/:path*"
    },
    {
      "source": "/savant-api/:path*",
      "destination": "https://baseballsavant.mlb.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
